pipeline {
    agent any

    stages {
        stage('Build_Git') {
            steps {
                echo 'Hello Anand, Build is ready'
            }
        }
        stage('Deploy_Git') {
            steps {
                echo 'We are go to deploy the code'
            }
        }
        stage('Tes_Git') {
            steps {
                echo 'Successfully tested!!'
            }
        }
        stage('Release_Git') {
            steps {
                echo 'Finally, we are ready to go on the Production..!!'
            }
        }        
    }
}
